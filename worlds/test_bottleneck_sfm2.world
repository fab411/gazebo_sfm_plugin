    <!-- ACTORS -->
    <!-- GROUP1 /////////////////////////////////////////////////////////////////////////////////////////// -->
    <!-- actor1 -->
    <actor name="actor1">
      <pose>-2 0 1.25 0 0 0</pose>
      <skin>
        <filename>walk.dae</filename>
        <scale>1.0</scale>
      </skin>
      <animation name="walking">
        <filename>walk.dae</filename>
        <scale>1.000000</scale>
        <interpolate_x>true</interpolate_x>
      </animation>
      <plugin name="actor1_plugin" filename="libPedestrianSFM2Plugin.so">
        <runge_kutta_45>false</runge_kutta_45>
        <sampling_time>0.033</sampling_time>
        <publish_forces>true</publish_forces>
        <topic_name>forces/actor1</topic_name>
        <node_name>Actor1</node_name>
        <velocity>0.9</velocity> 
        <radius>0.3</radius> 
        <mass>75</mass> 
        <relaxation_time>0.5</relaxation_time> 
        <k1g>200</k1g> 
        <k2g>200</k2g> 
        <Ai>2000</Ai> 
        <Aw>2000</Aw> 
        <Bi>0.08</Bi> 
        <Bw>0.08</Bw>
        <k1>120000</k1> 
        <k2>240000</k2> 
        <animation_factor>5.1</animation_factor>
        <animation_name>walking</animation_name>
        <people_distance>6.0</people_distance>
        <group>
            <model>actor2</model>
            <model>actor3</model>
            <model>actor4</model>
            <model>actor5</model>
            <model>actor6</model>
            <model>actor13</model>
            <model>actor14</model>
            <model>actor15</model>
            <model>actor16</model>
            <model>actor17</model>
            <model>actor18</model>
        </group>
        <ignore_obstacles>
          <model>ground_plane</model>
          <model>architrave_south</model>
        </ignore_obstacles>
        <trajectory>
          <cyclic>true</cyclic>
          <waypoint>-2 0 0</waypoint>
          <waypoint>0 0 0</waypoint>
          <waypoint>3 0 0</waypoint>
          <waypoint>10 0 0</waypoint>
        </trajectory>
      </plugin>
